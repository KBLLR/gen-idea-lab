# ==============================================
# SERVER-ONLY VARIABLES (NOT exposed to client)
# ==============================================

# AI API Keys (server-side only for security)
GOOGLE_API_KEY=your_google_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
CLAUDE_API_KEY=your_claude_api_key_here
OLLAMA_API_KEY=your_ollama_cloud_api_key_here

# Authentication secrets (server-side only)
AUTH_SECRET=your_random_secret_for_jwt_signing

# OAuth Client Secrets (server-side only for security)
GOOGLE_CLIENT_SECRET=your_google_oauth_client_secret
GITHUB_CLIENT_SECRET=your_github_oauth_client_secret
NOTION_CLIENT_SECRET=your_notion_oauth_client_secret
FIGMA_CLIENT_SECRET=your_figma_oauth_client_secret

# ==============================================
# CLIENT-SAFE VARIABLES (exposed to frontend via VITE_)
# ==============================================

# OAuth Client IDs (safe to expose - they're public identifiers)
VITE_GOOGLE_CLIENT_ID=your_google_oauth_client_id
VITE_GITHUB_CLIENT_ID=your_github_oauth_client_id
VITE_NOTION_CLIENT_ID=your_notion_oauth_client_id
VITE_FIGMA_CLIENT_ID=your_figma_oauth_client_id

# Application Configuration (safe to expose)
VITE_API_BASE_URL=http://localhost:8081
VITE_APP_NAME=GenBooth Idea Lab
VITE_APP_VERSION=1.0.0

# Environment-specific settings
VITE_NODE_ENV=development
VITE_ENABLE_DEBUG=true

# Server Configuration
PORT=8081
NODE_ENV=development