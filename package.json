{
  "name": "genbooth",
  "version": "1.0.0",
  "description": "GenBooth Application",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently -k -n client,server -c green,blue \"npm:dev:client\" \"npm:dev:server\"",
    "dev:client": "vite --port 3000",
    "dev:server": "nodemon server/index.js",
    "build": "npm run tokens:build && vite build",
    "vercel-build": "npm run tokens:build && vite build",
    "preview": "vite preview",
    "start": "node server/index.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "sb:build": "storybook build",
    "sb:test:ui": "playwright test .storybook/tests",
    "test:ui": "vitest --environment=jsdom",
    "test:ui:watch": "vitest --environment=jsdom --watch",
    "generate-thumbnails": "node scripts/generate-thumbnails.mjs",
    "tokens:build": "cd tokens && npx style-dictionary build --config config.json",
    "ds:check": "rg -n '\\d+px' src/design-system | (! read) || (echo 'px found in DS'; exit 1)",
    "tokens:watch": "cd tokens && npx style-dictionary build --watch",
    "tokens:clean": "rm -rf src/styles/tokens/tokens-*.css src/styles/tokens/tokens.js tokens/build",
    "tokens:audit": "node scripts/audit-css.js",
    "tokens:validate": "node scripts/validate-tokens.js",
    "test": "NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest",
    "test:auth": "NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest tests/auth.routes.test.js"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@gltf-transform/core": "^4.2.1",
    "@gltf-transform/extensions": "^4.2.1",
    "@gltf-transform/functions": "^4.2.1",
    "@google/genai": "^1.21.0",
    "@google/model-viewer": "^4.1.0",
    "@humeai/voice-react": "^0.2.5",
    "@mediapipe/tasks-vision": "^0.10.21",
    "@vladmandic/human": "^3.3.6",
    "auto-zustand-selectors-hook": "^3.0.1",
    "clsx": "^2.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "d3": "^7.9.0",
    "dompurify": "^3.2.7",
    "dotenv": "^16.4.5",
    "eventemitter3": "^5.0.1",
    "express": "^4.19.2",
    "google-auth-library": "^9.14.1",
    "googleapis": "^144.0.0",
    "immer": "^10.1.3",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "marked": "^16.3.0",
    "markmap-lib": "^0.18.12",
    "markmap-view": "^0.18.12",
    "mongodb": "^6.7.0",
    "multer": "^2.0.2",
    "nanoid": "^5.1.6",
    "node-fetch": "^3.3.2",
    "ollama": "^0.6.0",
    "p-limit": "^7.1.1",
    "prom-client": "^15.1.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.3.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.30.1",
    "reactflow": "^11.10.2",
    "simple-icons": "^13.0.0",
    "three": "^0.180.0",
    "winston": "^3.13.0",
    "ws": "^8.18.3",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@types/node": "^20.19.21",
    "@vitejs/plugin-react": "^4.3.0",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "storybook": "^8.6.14",
    "style-dictionary": "^5.0.4",
    "supertest": "^6.3.4",
    "typescript": "^5.9.3",
    "vite": "^5.4.0",
    "vitest": "^2.0.0"
  }
}
