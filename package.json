{
  "name": "genbooth",
  "version": "1.0.0",
  "description": "GenBooth Application",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently -k -n client,server -c green,blue \"npm:dev:client\" \"npm:dev:server\"",
    "dev:client": "vite --port 3000",
    "dev:server": "nodemon server.js",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node server.js",
    "generate-thumbnails": "node scripts/generate-thumbnails.mjs"
  },
  "dependencies": {
    "@google/genai": "^1.21.0",
    "auto-zustand-selectors-hook": "^3.0.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "immer": "^10.1.3",
    "p-limit": "^7.1.1",
    "prom-client": "^15.1.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "winston": "^3.13.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.0",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.0",
    "vite": "^5.4.0"
  }
}
